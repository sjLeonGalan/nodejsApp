pipeline{
    agent any
    stages{
        stage('one'){
            steps{
                echo 'stage 1'
            }
        }

        stage('two'){
            steps{
                echo 'stage 2'
            }
        }

        stage('three'){
            steps{
                echo 'stage 3'
            }
        }

        stage('four'){
            steps{
                echo 'stage 4'
            }
        }

        stage('five'){
            steps{
                echo 'stage 5'
            }
        }
    }
    
    post{
        always{
            echo 'pipeline finalizado'
        }
        success{
            echo 'Todo ha ido bien'
        }
        failure{
            echo 'Ha fallado todo'
        }
    }
}